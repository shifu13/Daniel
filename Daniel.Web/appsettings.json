{
  "$schema": "./appsettings-schema.json",
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=mssql2.dandomain.dk,1433;Database=d_bet_dk;User Id=d_bet;Password=ga4@ao7CQ."
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.ApplicationInsights"
    ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Ecreo.Essentials.UmbracoTemplate": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "ApplicationInsights",
        "Args": {
          "restrictedToMinimumLevel": "Information",
          "telemetryConverter": "Serilog.Sinks.ApplicationInsights.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "Sample"
    }
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "LoginBackgroundImage": "/img/login.jpg",
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true,
          "KeepAllVersionsNewerThanDays": 7,
          "KeepLatestVersionPerDayForDays": 90
        }
      },
      "Examine": {
        "LuceneDirectoryFactory": "SyncedTempFileSystemDirectoryFactory"
      },
      "Global": {
        "SanitizeTinyMce": true,
        "UseHttps": true,
        "MainDomLock": "FileSystemMainDomLock",
        "Id": "06bdc908-c9ad-4ca1-ba41-0d6d45b3b41a"
      },
      "Hosting": {
        "Debug": false,
        "LocalTempStorageLocation": "EnvironmentTemp"
      },
      "Imaging": {
        "Cache": {
          "BrowserMaxAge": "365.00:00:00",
          "CacheMaxAge": "365.00:00:00"
        }
      },
      "Runtime": {
        "Mode": "Production"
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "RuntimeMinification": {
        "useInMemoryCache": true,
        "CacheBuster": "Timestamp"
      },
      "Unattended": {
        "PackageMigrationsUnattended": true,
        "UpgradeUnattended": true
      }
    }
  }
}